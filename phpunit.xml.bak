<?xml version="1.0" encoding="UTF-8"?>
<phpunit colors="true">
    <testsuites>
        <testsuite name="Application Test">
            <directory>./phpUnitTutorial/Test</directory>
        </testsuite>
    </testsuites>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">./phpUnitTutorial/</directory>
        </whitelist>
    </filter>
</phpunit>